	<queryString>
		<![CDATA[select tbnf.nfnr, tbnf.nfvalor, tbnf.nfod, tbnf.nfdataemiss,
tbnf.nfdesconto, tbnf.pescodigo,tbnfiten.nfseq,prodes,
nfun,NFQT,(nfun * nfqt) as valor_total_item,unidade,
 tbpessoa.pesnome, (nfvalor - nfdesconto) as vlr_com_desconto
from tbnfiten
left outer join tbnf on tbnfiten.nfnr = tbnf.nfnr
left outer join tbproduto on  tbproduto.procod = tbnfiten.procod
left outer join tbunidade on tbunidade.uncod = tbproduto.uncod
left outer join tbpessoa on tbnf.pescodigo = tbpessoa.pescodigo
$P{filtroSql}]]>
	</queryString>
