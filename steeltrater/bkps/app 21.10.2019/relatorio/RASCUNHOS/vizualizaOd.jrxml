	<queryString>
		<![CDATA[select tbod.odcod,tbod.odaplic,tbod.odusercria,
tbod.oddatacria,tbod.odhoracria,tbod.condcod,tbcondpag.conddes,
tbod.odsitcod,tbodsit.odsitdes,tboditen.odseq,tboditen.procod,
tbproduto.prodes,tboditen.odqt,(tboditen.odpreco * tboditen.odqt)as unitario,
tboditen.odpreco,tboditen.cotcod,pesnome
from tbod left outer join tboditen
on tbod.odcod = tboditen.odcod left outer join tbcondpag
on tbod.condcod = tbcondpag.condcod left outer join tbodsit
on tbod.odsitcod = tbodsit.odsitcod left outer join tbproduto
on tboditen.procod = tbproduto.procod left outer join tbpessoa
on tbod.pescodigo = tbpessoa.pescodigo
$P{filtroSql}]]>
	</queryString>
