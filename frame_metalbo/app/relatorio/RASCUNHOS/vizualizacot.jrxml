	<queryString>
		<![CDATA[select tbcotiten.cotcod,tbcot.cotaplic,cotdataemi,cothoraemi,
cotusuario,tbcotiten.cotseq,
tbcotiten.procod,prodes,cotqt,tbcotiten.cotaplic as aplicIten,
pesnome,cotitenpreco,conddes,cotitenobs,(cotitenpreco*cotqt) as unitario,
cotsolnr,cotsoluser
from tbcotiten left outer join
tbcot on tbcotiten.cotcod = tbcot.cotcod left outer join
tbproduto on tbcotiten.procod = tbproduto.procod left outer join
tbcotitencomp on tbcotiten.cotcod = tbcotitencomp.cotcod
and tbcotiten.cotseq = tbcotitencomp.cotseq left outer join
tbpessoa on tbcotitencomp.pescodigo = tbpessoa.pescodigo left outer join
tbcondpag on tbcotitencomp.condcod = tbcondpag.condcod
$P{filtroSql}]]>
	</queryString>
